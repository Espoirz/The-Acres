{
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "DATABASE_URL": "@database_url",
    "NODE_ENV": "production"
  }
}